model websiteScrapperSite {
  id    String  @id @default(cuid()) @map("_id")
  site           String
  description    String
  status   String
  seoPercentage  Float
  dateAt        DateTime   @db.Date
 
}

 
 model websiteScrapperPage {
  id    String  @id @default(cuid()) @map("_id")
  site     String 
  data           Json
}
 model websiteScrapperImage {
  id    String  @id @default(cuid()) @map("_id")
  site     String 
  data           Json
}

 
